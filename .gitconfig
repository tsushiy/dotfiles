  email = 28962399+tsushiy@users.noreply.github.com

[alias]
  st = status
  b  = branch
  cm = commit -m
  rs = reset --soft HEAD^
  lg = log --graph --decorate=short --date=iso --pretty=format:'%C(green)%h %C(reset)[%cd] %C(blue)%cn %C(red)%d %C(reset)%s'
  pom = push origin master

[color]
  ui = auto

[core]
  excludesfile = ~/.gitignore_global

[url "git@github.com:"]
  pushInsteadOf = https://github.com/